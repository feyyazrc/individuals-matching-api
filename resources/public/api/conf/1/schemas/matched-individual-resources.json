{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Matched Individual",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "individual": {
          "type": "object",
          "description": "The URI for the resource to get the individual's available information.",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/matching/57072660-1df9-4aeb-b4ea-cd2d7f96e43"
            },
            "name": {
              "type": "string",
              "example": "GET"
            },
            "title": {
              "type": "string",
              "example": "Individual Details"
            }
          },
          "required": ["href"]
        },
        "self": {
          "type": "object",
          "description": "The URI to this resource",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/matching/"
            }
          },
          "required": ["href"]
        }
      },
      "required": ["individual", "self"]
    }
  },
  "required": ["_links"]
}
